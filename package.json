{
  "name": "new-movies",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://jesusgonzalez.dev/new-movies",
  "dependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.8.2",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^3.0.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "build-css": "node-sass src/assets/sass/main.scss -o src/assets/css/",
    "watch-css": "npm run build-css && node-sass src/assets/sass/main.scss -o src/assets/css/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5"
  }
}
